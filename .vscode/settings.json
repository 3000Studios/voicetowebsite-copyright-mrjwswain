{
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,

  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },

  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",

  "npm.packageManager": "npm",

  "search.exclude": {
    "**/dist": true,
    "**/node_modules": true
  },
  "editor.defaultFormatter": "google.antigravity-code-executor",
  "editor.definitionLinkOpensInPeek": true,
  "cSpell.words": [
    "ADSENSE",
    "Adsense",
    "AUTOINCREMENT",
    "Playfair",
    "Reshimming",
    "SAMEORIGIN",
    "Segoe",
    "VOICETOWEBSITE",
    "Voiceto",
    "adsbygoogle",
    "adsense",
    "appstore",
    "badsbygoogle",
    "buildhome",
    "ecommerce",
    "esbuild",
    "genai",
    "googlesyndication",
    "lucide",
    "massets",
    "mbuilding",
    "mdist",
    "mindex",
    "morphism",
    "mvite",
    "nosniff",
    "pagead",
    "playsinline",
    "protobufjs",
    "spectralize",
    "vite",
    "vitejs",
    "voicetowebsite",
    "webdev",
    "webforge",
    "workerd"
  ]
}
