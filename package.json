{
  "name": "voicetowebsite-copyright-mrjwswain",
  "private": true,
  "version": "56.3.0",
  "description": "Voice-to-Website: Autonomous Web Engineering & Monetization Engine",
  "author": "3000 Studios",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "dev:all": "node ./scripts/dev-all.mjs",
    "guard:deploy": "node ./scripts/guard-deploy.mjs",
    "governance:check": "node ./scripts/validate-governance.mjs",
    "ops:global-doc:update": "node ./scripts/update-global-system-doc.mjs",
    "ops:global-doc:check": "node ./scripts/update-global-system-doc.mjs --check",
    "generate:config": "node ./scripts/generate-config.mjs",
    "generate:sitemap": "node ./scripts/generate-sitemap.mjs",
    "prebuild": "npm run generate:config && npm run generate:sitemap",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "check:links": "node ./link-checker.js",
    "smash:check": "node ./check_smash.cjs",
    "smash:apply": "node ./apply_smash.cjs",
    "verify": "node ./scripts/verify.mjs && npm run guard:deploy && npm run ops:global-doc:check && npm run format:check && npm run type-check && npm run test && npm run build && npm run governance:check && npm run check:links",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check --ignore-unknown .",
    "heal": "node ./scripts/heal.mjs",
    "ship": "node ./scripts/ship.mjs",
    "ship:push": "node ./scripts/ship.mjs --push",
    "auto:ship": "node ./scripts/auto-ship.mjs",
    "auto:ship:install": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/install-auto-ship-task.ps1",
    "auto:ship:uninstall": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/uninstall-auto-ship-task.ps1",
    "sync": "node ./scripts/sync.mjs",
    "autopilot": "node ./scripts/autopilot.mjs",
    "prepare": "node ./scripts/husky-install.mjs",
    "clean": "node -e \"const fs=require('fs'); fs.rmSync('dist',{recursive:true,force:true});\"",
    "deploy": "npm run verify && wrangler deploy --keep-vars",
    "debug:tail": "wrangler tail",
    "debug:local": "DEBUG=vtw:* npm run dev"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx,css,scss,json,md,yml,yaml,html}": "prettier --write"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.2",
    "@cloudflare/workers-types": "^4.20250109.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.2",
    "@types/react": "^19.0.5",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "debug": "^4.4.3",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.1.6",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  },
  "dependencies": {
    "@google/genai": "^1.41.0",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.564.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
